    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Webpack",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/webpack/bin/webpack.js",
            "sourceMaps": true,
        },
        {
            "name": "Launch Program",
            "type": "node",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}\\dist\\bpmn.js",
        },
        {
            "name": "Launch chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}/dist",
            "sourceMaps": true,

        }
    ]
}